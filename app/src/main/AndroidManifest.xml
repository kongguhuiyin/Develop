<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wangyueke.news">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/tubiao"
        android:label="@string/app_name"
        android:roundIcon="@drawable/tubiao"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Browse"></activity>
        <activity android:name=".Registered"></activity>
        <activity android:name=".FindPassword"></activity>
        <activity android:name=".setting"></activity>
        <activity android:name=".ChangePassword"></activity>
        <activity android:name=".News"></activity>
    </application>

</manifest>